{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "memory-agent-starter",
  "main": "src/server.ts",
  "compatibility_date": "2025-01-01",
  "compatibility_flags": ["nodejs_compat"],
  "durable_objects": {
    "bindings": [
      {
        "name": "MEMORY_AGENT",
        "class_name": "MemoryAgent"
      }
    ]
  },
  "ai": {
    "binding": "AI"
  },
  "vectorize": [
    {
      "binding": "VECTORIZE",
      "index_name": "memory-embeddings"
    }
  ],
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["MemoryAgent"]
    }
  ]
}
